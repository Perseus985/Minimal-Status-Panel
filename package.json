{
  "name": "minimal-status-panel",
  "version": "1.2.0-beta.2",
  "description": "A Grafana panel plugin that displays status information similar to Uptime Kuma",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.js --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.js --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4 --coverage --watchAll=false",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "e2e": "npm exec playwright test",
    "e2e:update": "npm exec playwright test --update-snapshots",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest --rootUrls https://stellmacher.grafana.net",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Perseus985/Minimal-Status-Panel.git"
  },
  "author": "Your Name",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Perseus985/Minimal-Status-Panel/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@grafana/eslint-config": "^6.0.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.0",
    "@semantic-release/npm": "^12.0.0",
    "@swc/core": "^1.3.90",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^22.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.0.0",
    "eslint-webpack-plugin": "^5.0.0",
    "fork-ts-checker-webpack-plugin": "^9.0.0",
    "glob": "^11.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "prettier": "^3.0.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.93.2",
    "sass-loader": "16.0.5",
    "semantic-release": "^24.0.0",
    "style-loader": "4.0.0",
    "swc-loader": "^0.2.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.9.0",
    "webpack": "^5.86.0",
    "webpack-cli": "^6.0.0",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.4.1",
    "@grafana/data": "10.4.19",
    "@grafana/runtime": "10.4.19",
    "@grafana/ui": "10.4.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tslib": "2.8.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "packageManager": "npm@11.6.1"
}
